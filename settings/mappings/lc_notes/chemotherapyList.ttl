@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<Drug>
	rml:logicalSource [ rml:source "/data/data/lc_notes/chemotherapyList.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Drug/{ENG_CUI_Name}";
		rr:class clarify:Drug;
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/vocab/ChemotherapyDrug" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/vocab/LungCancerDrug" 
		]
	];			
	rr:predicateObjectMap [
		rr:predicate clarify:drugLabel_ENG;
		rr:objectMap [ 
			rml:reference "ENG_CUI_Name" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "{DBpediaURL}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasCUIAnnotation;
		rr:objectMap [
			rr:parentTriplesMap <Annotation>
		]
	].	

<Annotation>
	rml:logicalSource [ rml:source "/data/data/lc_notes/chemotherapyList.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Annotation/{CUI_ID}";
		rr:class clarify:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate clarify:annID;
		rr:objectMap [ 
			rml:reference "CUI_ID" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:annLabel_SPA;
		rr:objectMap [ 
			rml:reference "name" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:provenance;
		rr:objectMap [
			rr:template "http://project-iasis.eu/SourceName/UMLS"
		]
	].		
