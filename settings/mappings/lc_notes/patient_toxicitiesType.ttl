@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/data/data/lc_notes/patient_toxicitiesType.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/LCPatient/{patient_id}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasToxicityObservation;
		rr:objectMap [ 
			rr:parentTriplesMap <ToxicityObservation>
		]
	].	
<ToxicityObservation>
	rml:logicalSource [ rml:source "/data/data/lc_notes/patient_toxicitiesType.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/ToxicityObservation/{patient_id}_{toxicity_id}";
		rr:class clarify:ToxicityObservation;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasToxicity;
		rr:objectMap [ 
			rr:template "http://project-iasis.eu/Phenotype/{toxicity_concept}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasDate;
		rr:objectMap [ 
			rml:reference "date";
			rr:datatype xsd:date;
		]
	].
