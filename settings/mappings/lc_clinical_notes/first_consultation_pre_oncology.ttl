@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/data/data/lc_clinical_notes/First_consultation_pre_oncology.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/LCPatient/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasNonOncologicalConsultation;
		rr:objectMap [ 
			rr:parentTriplesMap <NonOncologicalConsultation>
		]
	].

<NonOncologicalConsultation>
	rml:logicalSource [ rml:source "/data/data/lc_clinical_notes/First_consultation_pre_oncology.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/NonOncologicalConsultation/{EHR}_{date_consultation}";
		rr:class clarify:NonOncologicalConsultation;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:date;
		rr:objectMap [
			rml:reference "date_consultation";
			rr:datatype xsd:date;
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:asymptomatic;
		rr:objectMap [
			rml:reference "asymptomatic";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:cough;
		rr:objectMap [
			rml:reference "cough";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:pain;
		rr:objectMap [
			rml:reference "pain";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:dyspnoea;
		rr:objectMap [
			rml:reference "dyspnoea";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hemoptysis;
		rr:objectMap [
			rml:reference "hemoptysis";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:asthenia;
		rr:objectMap [
			rml:reference "asthenia";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:anorexia;
		rr:objectMap [
			rml:reference "anorexy";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:weightLoss;
		rr:objectMap [
			rml:reference "weightLoss";		
		]
	].								