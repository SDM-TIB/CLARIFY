@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/data/data/lc_clinical_notes/synchronous_tumor.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/LCPatient/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:has_SynchronousTumor_OrNot;
		rr:objectMap [
			rr:template "http://clarify2020.eu/SynchronousTumor/{has_synchronous_tumor}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:description;
        rr:objectMap [ 
            rml:reference "description" 
        ]
	].								