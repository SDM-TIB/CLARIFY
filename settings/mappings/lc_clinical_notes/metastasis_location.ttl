@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<CancerStageTNM>
	rml:logicalSource [ rml:source "/mnt/e/data/data/slcg_notes/metastasis_location.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/CancerStageTNM/{EHR}_{ID}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:metastasized_to;
		rr:objectMap [ 
			rr:parentTriplesMap <MetastasisLocation>
		]
	].

<MetastasisLocation>
	rml:logicalSource [ rml:source "/mnt/e/data/data/slcg_notes/metastasis_location.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/MetastasisLocation/{EHR}_{ID}";
		rr:class clarify:MetastasisLocation;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_extrathoracicLymphadenopathies;
		rr:objectMap [
			rml:reference "extrathoracic_lymphadenopaties";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_thoracicLymphadenopathy;
		rr:objectMap [
			rml:reference "thoracic_lymphadenopaties";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_meningealCarcinomatosis;
		rr:objectMap [
			rml:reference "meningeal_carcinomatosis";
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_pericardialEffusion;
		rr:objectMap [
			rml:reference "pericardial_effusion";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_pleuralEffusion;
		rr:objectMap [
			rml:reference "pleural_effusion";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_liverMetastasis;
		rr:objectMap [
			rml:reference "liver";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_boneMetastasis;
		rr:objectMap [
			rml:reference "bone";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_bilateralLymphangitis;
		rr:objectMap [
			rml:reference "bilateral_lymphangitis";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_pleuralNodules;
		rr:objectMap [
			rml:reference "pleural_nodules";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_pancreasMetastasis;
		rr:objectMap [
			rml:reference "pancreatic";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_peritoneumMetastasis;
		rr:objectMap [
			rml:reference "peritoneal";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_lungMetastasis;
		rr:objectMap [
			rml:reference "lung";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_centralNervousSystemMetastasis;
		rr:objectMap [
			rml:reference "central_nervous_system";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_subcutaneousMetastasis;
		rr:objectMap [
			rml:reference "subcutaneous";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_adrenalMetastasis;
		rr:objectMap [
			rml:reference "adrenal";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:existenceOf_softTissuesMetastasis;
		rr:objectMap [
			rml:reference "soft_tissues";		
		]
	];
	rr:predicateObjectMap [
