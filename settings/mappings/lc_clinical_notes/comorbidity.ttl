@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/data/data/lc_clinical_notes/comorbidity_processed.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/LCPatient/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasComorbidity;
		rr:objectMap [ 
			rr:parentTriplesMap <Disorder>
		]
	].

<Disorder>
	rml:logicalSource [ rml:source "/data/data/lc_clinical_notes/comorbidity_processed.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/Disorder/{name}";
		rr:class clarify:Disorder;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:disorderLabel;
		rr:objectMap [
			rml:reference "name";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:hasCUIAnnotation;
		rr:objectMap [
			rr:parentTriplesMap <Annotation>
		]
	].	

<Annotation>
	rml:logicalSource [ rml:source "/data/data/lc_clinical_notes/comorbidity_processed.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/Annotation/{CUI}";
		rr:class clarify:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate clarify:annID;
		rr:objectMap [ 
			rml:reference "CUI" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:provenance;
		rr:objectMap [
			rr:template "http://clarify2020.eu/SourceName/UMLS"
		]
	].