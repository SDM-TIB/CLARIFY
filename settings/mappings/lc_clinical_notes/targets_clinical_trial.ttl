@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<ClinicalTrial>
	rml:logicalSource [ rml:source "/data/data/lc_clinical_notes/targets_clinical_trial.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/ClinicalTrial/{trial_name}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasTrialTarget;
		rr:objectMap [
			rr:parentTriplesMap <TrialTarget>
		]
	].			

<TrialTarget>
	rml:logicalSource [ rml:source "/data/data/lc_clinical_notes/targets_clinical_trial.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/TrialTarget/{target_name}";
		rr:class clarify:TrialTarget;
	].	