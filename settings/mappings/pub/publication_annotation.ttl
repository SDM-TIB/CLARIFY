@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<Publication>
	rml:logicalSource [ rml:source "/data/data/pub/Metamap_annotations_LC.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Publication/{subject}";
		rr:class iasis:Publication
	];
	rr:predicateObjectMap [
		rr:predicate iasis:pubmedID;
		rr:objectMap [ 
			rml:reference "subject" 
		]
	].					

<Annotation>
	rml:logicalSource [ rml:source "/data/data/pub/Metamap_annotations_LC.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Annotation/{object}";
		rr:class iasis:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate iasis:annID;
		rr:objectMap [ 
			rml:reference "object" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:semanticType;
		rr:objectMap [ 
			rml:reference "semantic_types" 
		]
	].

<HasAnnotation>
	rml:logicalSource [ rml:source "/data/data/pub/Metamap_annotations_LC.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/HAS_ANNOTATION/{subject}_HAS_ANNOTATION_{object}";
		rr:class iasis:HAS_ANNOTATION
	];
	rr:predicateObjectMap [
		rr:predicate iasis:confidence;
		rr:objectMap [ 
			rml:reference "confidence" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:hasAnnotation;
		rr:objectMap [
			rr:template "http://project-iasis.eu/Annotation/{object}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:annotates;
		rr:objectMap [
			rr:template "http://project-iasis.eu/Publication/{subject}"
		]
	].	