@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix bmLC: <http://bigmedilytics.eu/vocab/> .

<Target>
	rml:logicalSource [ rml:source "/data/data/drug/target.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://bigmedilytics.eu/Target/{Target_name}";
		rr:class bmLC:Target
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:targetLabel;
		rr:objectMap [
			rml:reference "Target_name";
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:targetID;
		rr:objectMap [
			rml:reference "Target_ID";
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "http://bigmedilytics.eu/Protein/{Uni_ID}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:uniprotID;
		rr:objectMap [
			rml:reference "Uni_ID";
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:externalLink;
		rr:objectMap [
			rr:template "https://www.uniprot.org/uniprot/{Uni_ID}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:target_isRelatedTo_gene;
		rr:objectMap [
			rr:template "http://bigmedilytics.eu/Gene/{Gene_name}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:hasCUIAnnotation;
		rr:objectMap [
			rr:parentTriplesMap <Annotation>
		]
	].

<Annotation>
	rml:logicalSource [ rml:source "/data/data/drug/target.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://bigmedilytics.eu/Annotation/{CUI}";
		rr:class bmLC:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:annID;
		rr:objectMap [ 
			rml:reference "CUI" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:provenance;
		rr:objectMap [
			rr:template "http://bigmedilytics.eu/SourceName/UMLS"
		]
	].	
