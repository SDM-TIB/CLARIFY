@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix bmLC: <http://bigmedilytics.eu/vocab/> .

<DrugEnzymeInteraction>
	rml:logicalSource [ rml:source "/data/data/drug/drug_enzyme_interaction.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://bigmedilytics.eu/DrugEnzymeInteraction/{Drug_ID}_interactsWith_{Enzyme_name}";
		rr:class bmLC:DrugEnzymeInteraction
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:interactor1;
		rr:objectMap [ 
			rr:template "http://bigmedilytics.eu/Drug/{Drug_ID}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:interactor2;
		rr:objectMap [ 
			rr:template "http://bigmedilytics.eu/Enzyme/{Enzyme_name}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:provenance;
		rr:objectMap [
			rr:template "http://bigmedilytics.eu/SourceName/KEGG"
		]
	].
