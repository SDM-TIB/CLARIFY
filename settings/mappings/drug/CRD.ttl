@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<DrugDrugInteraction>
	rml:logicalSource [ rml:source "/mnt/e/data/data/drug/CRD.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/DrugDrugInteraction/{Drug1}_interactsWith_{Drug2}";
		rr:class clarify:DrugDrugInteraction
	];
	rr:predicateObjectMap [
		rr:predicate clarify:affects;
		rr:objectMap [ 
			rr:template "http://clarify2020.eu/Drug/{Drug1}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:isAffected;
		rr:objectMap [ 
			rr:template "http://clarify2020.eu/Drug/{Drug2}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:provenance;
		rr:objectMap [
			rr:template "http://clarify2020.eu/SourceName/DrugBank/metabolized_by_the_same_CYP_enzyme"
		]
	].
