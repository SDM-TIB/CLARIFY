@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix bmLC: <http://bigmedilytics.eu/vocab/> .

<DrugTargetInteraction>
	rml:logicalSource [ rml:source "/data/data/drug/drugTargetInteraction.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://bigmedilytics.eu/DrugTargetInteraction/{Drug}_interactsWith_{Protein ID}";
		rr:class bmLC:DrugTargetInteraction
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:interactor1;
		rr:objectMap [ 
			rr:template "http://bigmedilytics.eu/Drug/{Drug}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:interactor2;
		rr:objectMap [ 
			rr:parentTriplesMap <Target> 
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:provenance;
		rr:objectMap [
			rr:template "http://bigmedilytics.eu/SourceName/DrugBank"
		]
	].
		
<Target>
	rml:logicalSource [ rml:source "/data/data/drug/drugTargetInteraction.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://bigmedilytics.eu/Target/{Protein ID}";
		rr:class bmLC:Target
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:targetID;
		rr:objectMap [
			rml:reference "Protein ID"
		]
	];		
	rr:predicateObjectMap [
		rr:predicate bmLC:targetLabel;
		rr:objectMap [
			rml:reference "Protein Name"
		]
	];		
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [ 
			rr:parentTriplesMap <Protein>
		]
	].

<Protein>
	rml:logicalSource [ rml:source "/data/data/drug/drugTargetInteraction.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://bigmedilytics.eu/Protein/{Protein ID}";
		rr:class bmLC:Protein
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:proteinID;
		rr:objectMap [
			rml:reference "Protein ID"
		]
	];	
	rr:predicateObjectMap [
		rr:predicate bmLC:proteinLabel;
		rr:objectMap [
			rml:reference "Protein Name"
		]
	];		
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [ 
			rr:template "http://bigmedilytics.eu/Protein/{Protein ID}"; 
		]
	].	
