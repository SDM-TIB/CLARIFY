@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix bmLC: <http://bigmedilytics.eu/vocab/> .

<DrugDrugPrediction>
	rml:logicalSource [ rml:source "/mnt/e/data/data/drug_predictions/DDI_prediction_LUH.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://bigmedilytics.eu/DrugDrugPrediction/{Drug1}_interactsWith_{Drug2}";
		rr:class bmLC:DrugDrugPrediction
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:interactor1;
		rr:objectMap [ 
			rr:template "http://bigmedilytics.eu/Drug/{Drug1}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:interactor2;
		rr:objectMap [ 
			rr:template "http://bigmedilytics.eu/Drug/{Drug2}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:confidence;
		rr:objectMap [ 
			rml:reference "Prob" 
		]
	];	
	rr:predicateObjectMap [
		rr:predicate bmLC:provenance;
		rr:objectMap [
			rr:template "http://bigmedilytics.eu/SourceName/{Source}"
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:predictionMethod;
		rr:objectMap [ 
			rml:reference "Method" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:methodAbbreviation;
		rr:objectMap [ 
			rml:reference "MethodAbbreviation" 
		]
	].			
		
