@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix bmLC: <http://bigmedilytics.eu/vocab/> .

<DrugDrugInteraction>
	rml:logicalSource [ rml:source "/data/data/drug/NCRD.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://bigmedilytics.eu/DrugDrugInteraction/{Drug1}_interactsWith_{Drug2}";
		rr:class bmLC:DrugDrugInteraction
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:affects;
		rr:objectMap [ 
			rr:template "http://bigmedilytics.eu/Drug/{Drug1}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:isAffected;
		rr:objectMap [ 
			rr:template "http://bigmedilytics.eu/Drug/{Drug2}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:provenance;
		rr:objectMap [
			rr:template "http://bigmedilytics.eu/SourceName/DrugBank/not_metabolized_by_the_same_CYP_enzyme"
		]
	].
