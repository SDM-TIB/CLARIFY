@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix bmLC: <http://bigmedilytics.eu/vocab/> .
<Disorder>
	rml:logicalSource [ rml:source "/data/data/drug/indication_CUI.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://bigmedilytics.eu/Disorder/{name}";
		rr:class bmLC:Disorder
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:disorderLabel;
		rr:objectMap [ 
			rml:reference "name" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "{Dbpedia_URL}";
		]
	];		
	rr:predicateObjectMap [
		rr:predicate bmLC:hasCUIAnnotation;
		rr:objectMap [
			rr:parentTriplesMap <Annotation>
		]
	].

<Annotation>
	rml:logicalSource [ rml:source "/data/data/drug/indication_CUI.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://bigmedilytics.eu/Annotation/{ID}";
		rr:class bmLC:Annotation
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:annID;
		rr:objectMap [ 
			rml:reference "ID" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate bmLC:provenance;
		rr:objectMap [
			rr:template "http://bigmedilytics.eu/SourceName/UMLS"
		]
	].

