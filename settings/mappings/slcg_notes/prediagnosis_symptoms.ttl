@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/data/data/slcg_notes/prediagnosis_symptoms.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/LCPatient/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasPrediagnosis;
		rr:objectMap [ 
			rr:parentTriplesMap <Prediagnosis>
		]
	].

<Prediagnosis>
	rml:logicalSource [ rml:source "/data/data/slcg_notes/prediagnosis_symptoms.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/Prediagnosis/{EHR}_{ID}";
		rr:class clarify:Prediagnosis;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:fever;
		rr:objectMap [
			rml:reference "fever";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:weightLoss;
		rr:objectMap [
			rml:reference "weightLoss";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:sleepingDifficulty;
		rr:objectMap [
			rml:reference "sleep";

		]
	].		