@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/data/data/slcg_notes/environmental_risk.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/LCPatient/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:exposed_to;
		rr:objectMap [ 
			rr:parentTriplesMap <Carcinogen>
		]
	].

<Carcinogen>
	rml:logicalSource [ rml:source "/data/data/slcg_notes/environmental_risk.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/Carcinogen/{EHR}_{ID}";
		rr:class clarify:Carcinogen;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:occupation;
		rr:objectMap [
			rml:reference "occupation";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:occupationDuration;
		rr:objectMap [
			rml:reference "years_in_occupation";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:mostProbable;
		rr:objectMap [
			rml:reference "probable_carcinogen";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:asbestos;
		rr:objectMap [
			rml:reference "asbestos_carcinogen";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:arsenic;
		rr:objectMap [
			rml:reference "arsenic_carcinogen";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:radon;
		rr:objectMap [
			rml:reference "radon_carcinogen";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:silica;
		rr:objectMap [
			rml:reference "silice_carcinogen";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:dieselSmoke;
		rr:objectMap [
			rml:reference "smoke_carcinogen";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:asbestosExposure;
		rr:objectMap [
			rml:reference "asbestos_exposition";		
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:asbestosExposureType;
		rr:objectMap [
			rml:reference "asbestos_exposition_type";		
		]
	].								