@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/data/data/slcg_notes/pathological_stadification_techniques.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/LCPatient/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasPathologicalStageClassification;
		rr:objectMap [ 
			rr:parentTriplesMap <PathologicalStageClassification>
		]
	].

<PathologicalStageClassification>
	rml:logicalSource [ rml:source "/data/data/slcg_notes/pathological_stadification_techniques.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/PathologicalStageClassification/{EHR}_{ID}";
		rr:class clarify:PathologicalStageClassification;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:biopsy;
		rr:objectMap [
			rml:reference "biopsy";		
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:broncoscopy;
		rr:objectMap [
			rml:reference "broncoscopy";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:ebus;
		rr:objectMap [
			rml:reference "endobronquial";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:laparoscopy;
		rr:objectMap [
			rml:reference "laparoscopy";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:fineNeedleAspiration;
		rr:objectMap [
			rml:reference "fine_needle_aspiration";		
		]
	];			
	rr:predicateObjectMap [
		rr:predicate clarify:bloodSample;
		rr:objectMap [
			rml:reference "blood_sample";		
		]
	];		
	rr:predicateObjectMap [
		rr:predicate clarify:mediastinoscopy;
		rr:objectMap [
			rml:reference "mediatinoscopy";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:transthoracicVats;
		rr:objectMap [
			rml:reference "thoracoscopy_vats";		
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:transthoracicPuncture;
		rr:objectMap [
			rml:reference "transthoracic_puncture";		
		]
	].
																