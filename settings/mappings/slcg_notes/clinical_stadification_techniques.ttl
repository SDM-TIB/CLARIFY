@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/data/data/slcg_notes/clinical_stadification_techniques.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/LCPatient/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasClinicalStageClassification;
		rr:objectMap [ 
			rr:parentTriplesMap <ClinicalStageClassification>
		]
	].

<ClinicalStageClassification>
	rml:logicalSource [ rml:source "/data/data/slcg_notes/clinical_stadification_techniques.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/ClinicalStageClassification/{EHR}_{ID}";
		rr:class clarify:ClinicalStageClassification;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:fullBloodCount;
		rr:objectMap [
			rml:reference "fbc";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:gammaScan;
		rr:objectMap [
			rml:reference "bone_gammagraphy";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:mediastinoscopy;
		rr:objectMap [
			rml:reference "mediatinoscopy";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:mediastinotomy;
		rr:objectMap [
			rml:reference "mediatinostomy";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:petTAC;
		rr:objectMap [
			rml:reference "pet_tac";		
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:thoracicRX;
		rr:objectMap [
			rml:reference "rx_thorax";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:brainMRI;
		rr:objectMap [
			rml:reference "brain_mri";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:mri;
		rr:objectMap [
			rml:reference "mri";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:thoracoscopy;
		rr:objectMap [
			rml:reference "torachoscopy";		
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:thoracotomy;
		rr:objectMap [
			rml:reference "thoracotomy";		
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:brainCT;
		rr:objectMap [
			rml:reference "brain_ct";		
		]
	].
