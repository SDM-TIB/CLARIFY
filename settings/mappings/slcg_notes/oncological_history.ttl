@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix clarify: <http://clarify2020.eu/vocab/> .

<LCPatient>
	rml:logicalSource [ rml:source "/data/data/slcg_notes/oncological_history.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/LCPatient/{EHR}";
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasPatientHistory;
		rr:objectMap [ 
			rr:parentTriplesMap <PatientHistory>
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:hasFamilyHistory;
		rr:objectMap [ 
			rr:parentTriplesMap <FamilyHistory>
		]
	].

<PatientHistory>
	rml:logicalSource [ rml:source "/data/data/slcg_notes/oncological_history.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/PatientHistory/{ID}";
		rr:class clarify:PatientHistory;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:previousCancer;
		rr:objectMap [
			rml:reference "previous_cancers";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:previousCancerType;
		rr:objectMap [
			rml:reference "previous_cancer_type";
		]
	].			

<FamilyHistory>
	rml:logicalSource [ rml:source "/data/data/slcg_notes/oncological_history.csv";
						rml:referenceFormulation ql:CSV;
						];
	rr:subjectMap [
		rr:template "http://clarify2020.eu/FamilyHistory/{ID}";
		rr:class clarify:FamilyHistory;
	];
	rr:predicateObjectMap [
		rr:predicate clarify:familyCancerType;
		rr:objectMap [
			rml:reference "family_cancer_antecedents";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate clarify:numberOfFamilyWithLC;
		rr:objectMap [
			rml:reference "number_of_family_members_cancer";
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:numberOfFamilyWithOtherCancers;
		rr:objectMap [
			rml:reference "number_of_family_members_others";
		]
	];
	rr:predicateObjectMap [
		rr:predicate clarify:familyType;
		rr:objectMap [
			rml:reference "relationship";
		]
	].	